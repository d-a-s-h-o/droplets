server {
    listen 80 default_server;
    root /var/www/html/.files/sites/0;
    index index.php index.html index.htm;
    server_name 0.site;

    location / {
        try_files $uri $uri/ =404;
    }

    location ~ \.php$ {
        include snippets/fastcgi-php.conf;
        fastcgi_pass unix:/var/run/php/php7.4-fpm.sock;
    }
}

